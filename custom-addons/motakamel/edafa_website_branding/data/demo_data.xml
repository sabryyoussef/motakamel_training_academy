        <!-- Demo Admission Register for Testing -->
        <record id="demo_admission_register_2025" model="op.admission.register">
            <field name="name">Demo Online Applications 2025</field>
            <field name="start_date" eval="(DateTime.now() - timedelta(days=30)).strftime('%Y-%m-%d')"/>
            <field name="end_date" eval="(DateTime.now() + timedelta(days=60)).strftime('%Y-%m-%d')"/>
            <field name="max_count">1000</field>
            <field name="min_count">1</field>
            <field name="product_id" ref="product_application_fee"/>
            <field name="state">confirm</field>
            <field name="admission_base">course</field>
        </record>
        
        <!-- Demo Partner 1 -->
        <record id="demo_partner_ahmed" model="res.partner">
